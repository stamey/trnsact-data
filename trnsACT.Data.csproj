<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>trnsACT.Data</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Joel Stamey</Authors>
    <Product>trnsACT</Product>
    <Version>1.0.0</Version>
    <Description>Base data classes and abstractions for trnsACT enterprise solution.</Description>
    <Copyright>© Joel Stamey 2021</Copyright>
    <PackageIcon>trnsact_128.png</PackageIcon>
    <PackageProjectUrl>https://github.com/stamey/trnsACT2022</PackageProjectUrl>
    <RepositoryUrl>https://github.com/stamey/trnsact-data</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
      <None Include="assets\trnsact_128.png" Pack="true" PackagePath=""/>
  </ItemGroup>

</Project>
